		} else {
			fprintf(stdout, "Operation done successfully\n");
		}
		sqlite3_close(db);
		adminMenu2();
	}
		break;
	case 8: {
		adminMenu();
	}
		break;
	default: {
		cout << "Introduzca un valor correcto" << endl;
		adminMenu2();
	}
		break;
	}

}