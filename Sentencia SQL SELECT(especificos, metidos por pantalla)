		/* Create SQL statement */
		char sql[] =
				"SELECT * from RESTAURANTE_TIENE_TRABAJADORES WHERE ID_RESTAURANTE = ";

		strcat(sql, nom);

		char fr2[] = "";

		strcat(sql, fr2);

